# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'Swift Food' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Swift Food

  pod 'RxSwift', '~> 5.0'
  pod 'RxCocoa', '~> 5.0'
  pod 'Alamofire', '~> 5.2'
  pod 'RxAlamofire', '~> 5.3'
  pod 'Moya/RxSwift', '~> 14.0'
  pod 'Resolver', '~> 1.1.4'
  pod 'Kingfisher', '~> 5.15.6'

  target 'Swift FoodTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RxBlocking', '~> 5.0'
    pod 'RxTest', '~> 5.0'
  end

  target 'Swift FoodUITests' do
    # Pods for testing
    pod 'RxBlocking', '~> 5.0'
    pod 'RxTest', '~> 5.0'
  end

end
